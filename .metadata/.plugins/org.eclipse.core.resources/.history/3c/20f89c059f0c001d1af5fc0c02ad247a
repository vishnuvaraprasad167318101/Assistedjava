
public class car {

	int speed=200;
	void display() {
		System.out.println("this is a car");
	}
}
class BMW07 extends car{
	int speed=300;
	void speedOfBMW07() {
		System.out.println(speed+120);
	}
}
class AUdinewBMW07 extends R15{
	void speedofnewBMW07() {
		System.out.println(speed+200);
	}
}
public class carEx {
public static void main(String[] args) {
	AUdinewBMW07 bmw07=new AUdinewBMW07();
	bmw07.speedofnewBMW07();
	bmw07.display();

}
}

